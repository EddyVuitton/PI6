<MudCarousel Style="height:200px; border-radius: 20px;" ShowArrows="@arrows" ShowBullets="@bullets" AutoCycle="@autocycle" TData="object">
    <MudCarouselItem Transition="transition" Color="@Color.Info">
        <MudIcon Class="mx-auto my-auto" Size="@Size.Large" />
    </MudCarouselItem>
    <MudCarouselItem Transition="transition" Color="@Color.Success">
        <MudIcon Class="mx-auto my-auto" Size="@Size.Large" />
    </MudCarouselItem>
    <MudCarouselItem Transition="transition" Color="@Color.Secondary">
        <MudIcon Class="mx-auto my-auto" Size="@Size.Large" />
    </MudCarouselItem>
</MudCarousel>